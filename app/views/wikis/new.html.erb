<% content_for :header do -%>
  Nova página do Projeto <% @project.name %>
<% end -%>


<% form_for([@project, @wiki]) do |f| %>
  <%= f.error_messages %>
  <%= f.hidden_field(:project_id) %>


  <p>
    <%= f.label :title, "Título" %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :body, "Texto" %><br />
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.submit "Criar" %>
  </p>
<% end %>


<%= link_to 'voltar', project_wikis_path(@project) %>
